[user]
	name = Neel Yadav
	email = mail@neelyadav.com
	signingkey = 2EE08C445EE9CB7F47774138ED84CBAAA8A7B576
[pull]
	rebase = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = master
[commit]
	gpgsign = true
[credential]
	helper = osxkeychain
